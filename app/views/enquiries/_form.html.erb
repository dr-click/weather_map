<%= form_with(model: enquiry, local: true) do |form| %>
  <% if enquiry.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(enquiry.errors.count, "error") %> prohibited this enquiry from being saved:</h2>

      <ul>
      <% enquiry.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :country %>
    <%= form.text_field :country, id: :enquiry_country %>
  </div>

  <div class="field">
    <%= form.label :city %>
    <%= form.text_field :city, id: :enquiry_city %>
  </div>

  <div class="field">
    <%= form.label :results %>
    <%= form.text_field :results, id: :enquiry_results %>
  </div>

  <div class="field">
    <%= form.label :last_update_at %>
    <%= form.datetime_select :last_update_at, id: :enquiry_last_update_at %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
